<h1>Wikis</h1>
<div class="col-md-8">
  <% @wikis.each do |wiki| %>
    <div class="media">
      <div class="media-body">
        <div class="media-heading">
          <%= link_to wiki.title, wiki %>
        </div>
      </div>
    </div>
  <% end %>
</div>
<div class="col-md-4">
  <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
</div>
